{
    "name": "Sub Grouping",
    "manifest_version": 3,
    "version": "1.0.0",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "description": "Create, delete, and customize tab sub-groups to better organize and clean your browser experience.",
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "permissions": [
        "scripting",
        "tabGroups",
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}